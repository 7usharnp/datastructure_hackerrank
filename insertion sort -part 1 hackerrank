#include <bits/stdc++.h>
using namespace std;


void display(int arr[],int n)
{
    for(int i=0;i<n;i++)
    {
        cout<<arr[i]<<" ";
    }
    cout<<endl;
}

void sort(int arr[],int n)
{
    int key,i,j;
    for( i=1;i<n;i++)
    {
        key=arr[i];
        j=i-1;
    
    
    while(j>=0 && arr[j]>key)
    {
        arr[j+1]=arr[j];
        j=j-1;
    }
    arr[j+1]=key;
    display(arr,n);
    }
    
}







int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    sort(arr,n);
   
}
